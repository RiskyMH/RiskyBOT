{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "build:ci": {
            "dependsOn": [
                "^build:ci"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ],
            "outputs": []
        }
    }
}