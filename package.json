{
  "name": "riskybot",
  "description": "A random multi-purpose bot",
  "version": "4.0.0",
  "author": "RiskyMH",
  "dependencies": {
    "@discordjs/builders": "^0.14.0-dev.1652443433-d522320",
    "@discordjs/rest": "^0.4.1",
    "@riskybot/apis": "./packages/apis",
    "@riskybot/functions": "./packages/functions",
    "@riskybot/tools": "./packages/tools",
    "discord-api-types": "^0.32.1",
    "discord.js": "^14.0.0-dev.1652443445-d522320",
    "dotenv": "^16.0.1",
    "node-fetch": "^3.2.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@riskybot/website": "file:./website",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "jshint": "^2.13.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=17.0.0"
  },
  "files": [
    "src/"
  ],
  "homepage": "https://riskymh.github.io/RiskyBOT",
  "main": "src/main.js",
  "module": "system",
  "private": true,
  "scripts": {
    "build": "tsc",
    "deployCommands": "node -r dotenv/config src/applicationCommands/deploy.js",
    "deployCommands:extra": "node -r dotenv/config src/applicationCommands/deployExtra.js",
    "deployCommands:ownerOnly": "node -r dotenv/config src/applicationCommands/deployOwnerOnly.js",
    "lint": "eslint ./src/* ./packages/functions/src/* ./packages/tools/src/* ./packages/apis/src/* ",
    "start": "node -r dotenv/config dist/src/main.mjs",
    "start:force": "(yarn --force && yarn build || echo ) && yarn start",
    "start:extra": "node -r dotenv/config dist/src/extra.mjs",
    "website:dev": "cd website && yarn serve:dev",
    "build:website": "cd website && yarn build"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ]
}
