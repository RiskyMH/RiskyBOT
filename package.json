{
  "name": "riskybot",
  "description": "A random multi-purpose bot",
  "version": "4.0.0",
  "author": "RiskyMH",
  "dependencies": {
    "@discordjs/builders": "^1.1.0",
    "@discordjs/rest": "^1.0.1",
    "@riskybot/apis": "workspace:^",
    "@riskybot/commands": "workspace:^",
    "@riskybot/discord-interactions": "workspace:^",
    "@riskybot/tools": "workspace:^",
    "body-parser": "^1.20.0",
    "discord-api-types": "^0.37.0",
    "dotenv": "^16.0.1",
    "turbo": "^1.4.0",
    "undici": "^5.8.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "jshint": "^2.13.5",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "src/"
  ],
  "homepage": "https://riskymh.github.io/RiskyBOT",
  "main": "dist/main.mjs",
  "module": "system",
  "private": true,
  "scripts": {
    "build": "turbo run build && tsc",
    "update": "yarn upgrade-interactive",
    "deployCommands": "node -r dotenv/config dist/applicationCommands/deploy.mjs",
    "deployCommands:ownerOnly": "node -r dotenv/config dist/applicationCommands/deployOwnerOnly.mjs",
    "lint": "eslint src/*.mts && turbo run lint",
    "start": "node dist/main.mjs"
  },
  "type": "module",
  "packageManager": "yarn@3.2.2",
  "workspaces": [
    "packages/*"
  ]
}
