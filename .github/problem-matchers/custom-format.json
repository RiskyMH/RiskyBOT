{
    "problemMatcher": [
        {
            "owner": "custom-format",
            "pattern": [
                {
                    "regexp": "\/([^\\\\s].*)[\\\\(:](\\d+):(\\d+)(?::\\s+|\\s+-\\s+)(error|warning|info)\\s*:\\s*((.*)\\s*\\((TS\\d+)\\))$\/gm",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 7
                }
            ]
        }
    ]
}