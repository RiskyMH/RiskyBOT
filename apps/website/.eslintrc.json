{
    "extends": [
        "plugin:@next/next/recommended",
        "../../.eslintrc.json"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@next/next/no-img-element": "off",
        "no-constant-binary-expression": "off"
    }
}
