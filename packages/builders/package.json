{
    "name": "@riskybot/builders",
    "description": "JSX support and builders for the RiskyBOT project",
    "version": "5.0.0",
    "author": "RiskyMH",
    "homepage": "https://github.com/RiskyMH/RiskyBOT/tree/main/packages/builders",
    "repository": {
        "type": "git",
        "url": "https://github.com/RiskyMH/RiskyBOT.git",
        "directory": "packages/builders"
    },
    "scripts": {
        "build": "bun ../../scripts/build.ts",
        "lint": "eslint src/**/*.ts tests/*.ts",
        "prepack": "yarn build",
        "test": "node --test"
    },
    "dependencies": {
        "@lilybird/jsx": "^0.2.0",
        "lilybird": "^0.4.4"
    },
    "devDependencies": {
        "eslint": "^8.56.0",
        "typescript": "^5.3.3"
    },
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "bun": "./src/index.ts",
            "types": "./src/index.ts",
            "import": "./dist/index.js"
        },
        "./jsx-runtime": {
            "bun": "./src/runtime.tsx",
            "types": "./src/runtime.tsx",
            "import": "./dist/runtime.jsx"
        },
        "./jsx-dev-runtime": {
            "bun": "./src/runtime.tsx",
            "types": "./src/runtime.tsx",
            "import": "./dist/runtime.jsx"
        }
    },
    "sideEffects": false,
    "directories": {
        "lib": "src"
    },
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    }
}
