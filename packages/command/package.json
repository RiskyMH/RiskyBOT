{
    "name": "@riskybot/command",
    "description": "The command handler for RiskyBOT",
    "version": "5.0.0",
    "homepage": "https://github.com/RiskyMH/RiskyBOT/tree/main/packages/command",
    "repository": {
        "type": "git",
        "url": "https://github.com/RiskyMH/RiskyBOT.git",
        "directory": "packages/command"
    },
    "scripts": {
        "build": "bun ../../scripts/build.ts",
        "lint": "eslint src/**/*.ts",
        "prepack": "yarn build"
    },
    "bin": {
        "command-imports": "./dist/compile-imports.mjs"
    },
    "dependencies": {
        "@discordjs/builders": "^1.7.0",
        "@riskybot/error": "workspace:^",
        "@riskybot/tools": "workspace:^",
        "discord-api-parser": "workspace:^",
        "discord-api-types": "^0.37.66"
    },
    "devDependencies": {
        "eslint": "^8.55.0",
        "typescript": "^5.3.3"
    },
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./src/index.ts",
    "exports": {
        "bun": "./src/index.ts",
        "types": "./src/index.ts",
        "import": "./dist/index.js"
    },
    "sideEffects": false,
    "directories": {
        "lib": "src"
    },
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    }
}
